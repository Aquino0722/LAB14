{
  "buildCommand": "dotnet publish LabLINQ.csproj -c Release -o ./published",
  "outputDirectory": "published",
  "framework": null,
  "installCommand": "dotnet restore LabLINQ.csproj",
  "devCommand": "dotnet watch --project LabLINQ.csproj run",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/$1"
    }
  ],
  "env": {
    "ASPNETCORE_ENVIRONMENT": "Production"
  }
}

